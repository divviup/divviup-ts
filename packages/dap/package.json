  },
  "devDependencies": {
    "@hpke/chacha20poly1305": "^1.5.0",
    "@hpke/core": "^1.7.1",
    "@hpke/dhkem-x25519": "^1.5.0",
    "esbuild": "^0.25.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./browser": "./dist/browser.js"
  }
