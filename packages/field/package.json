{
  "name": "@divviup/field",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "browser": "dist/browser.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:clean": "npm run clean && npm run build",
    "build": "parcel build",
    "build:node": "tsc -p ./tsconfig.json",
    "build:browser": "esbuild ./src/index.ts --bundle  --outfile=dist/browser.js --sourcemap",
    "docs": "typedoc src",
    "test": "ts-mocha \"src/**/*.spec.ts\" -p ./tsconfig.json",
    "lint": "eslint src --ext .ts && prettier -c src",
    "format": "prettier -w src",
    "test:coverage": "nyc --cwd=../.. --no-clean --reporter=lcov --reporter=text --reporter=json npm test",
    "ci": "npm run lint && npm run test:coverage"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "@divviup/common": "../common",
    "one-webcrypto": "^1.0.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tsconfig/node18": "^1.0.1",
    "@types/chai": "^4.3.3",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typedoc": "^0.23.15",
    "typescript": "=4.8.4",
    "typescript-eslint-language-service": "^5.0.0"
  }
}
